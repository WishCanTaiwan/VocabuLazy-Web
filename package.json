{
  "name": "voabulazy-web",
  "version": "0.0.1",
  "description": "VocabuLazy web version.",
  "author": "JerryZGY",
  "license": "MIT",
  "homepage": "https://github.com/WishCanTaiwan/VocabuLazy-Web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WishCanTaiwan/VocabuLazy-Web.git"
  },
  "bugs": {
    "url": "https://github.com/WishCanTaiwan/VocabuLazy-Web/issues"
  },
  "keywords": [
    "vocabulazy"
  ],
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "start": "yarn build && clear && node build/server.js",
    "build": "yarn del && yarn client:build && yarn server:build",
    "client": "node scripts/start.js",
    "client:build": "node scripts/build.js",
    "server": "yarn server:build && clear && cross-env NODE_ENV=development node build/server.js",
    "server:build": "webpack --config config/webpack.config.server.js",
    "del": "rimraf build/*",
    "test": "jest --coverage --forceExit || true"
  },
  "proxy": "http://localhost/",
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "jest": {
    "mapCoverage": true,
    "testRegex": "/src/tests/.*\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "history": "^4.7.2",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.0.1",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.0",
    "koa-static": "^4.0.1",
    "object-assign": "^4.1.1",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth2": "^0.1.6",
    "promise": "^8.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.74.2"
  },
  "devDependencies": {
    "@types/history": "^4.6.0",
    "@types/jest": "^21.1.2",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.24",
    "@types/koa-logger": "^2.0.2",
    "@types/koa-passport": "^2.0.4",
    "@types/koa-router": "^7.0.23",
    "@types/koa-session": "^3.0.4",
    "@types/koa-static": "^3.0.1",
    "@types/node": "^8.0.26",
    "@types/passport-facebook": "^2.1.5",
    "@types/passport-google-oauth2": "^0.1.1",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^16.0.1",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^5.0.6",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-router-redux": "^5.0.8",
    "@types/webpack": "^3.0.10",
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^21.0.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "react-dev-utils": "^4.0.1",
    "react-error-overlay": "^2.0.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-jest": "^21.0.0",
    "ts-loader": "^2.3.4",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "^1.3.1",
    "webpack-node-externals": "^1.6.0"
  }
}
