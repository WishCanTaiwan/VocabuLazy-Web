{
  "name": "voabulazy-web",
  "version": "0.0.1",
  "description": "VocabuLazy web version.",
  "author": "JerryZGY",
  "license": "MIT",
  "homepage": "https://github.com/WishCanTaiwan/VocabuLazy-Web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WishCanTaiwan/VocabuLazy-Web.git"
  },
  "bugs": {
    "url": "https://github.com/WishCanTaiwan/VocabuLazy-Web/issues"
  },
  "keywords": [
    "vocabulazy"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "yarn build && clear && node build/server.js",
    "build": "yarn del && yarn client:build && yarn server:build",
    "client": "node scripts/start.js",
    "client:build": "node script/build.js",
    "server": "yarn server:build && clear && cross-env NODE_ENV=development node build/server.js",
    "server:build": "webpack --config config/webpack.config.server.js",
    "del": "del-cli build",
    "test": "jest --coverage --forceExit || true"
  },
  "proxy": "http://localhost/",
  "babel": {

    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "jest": {
    "mapCoverage": true,
    "testRegex": "/src/tests/.*\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.0.0",
    "koa-passport": "^3.0.0",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.0",
    "koa-static": "^4.0.1",
    "passport-google-oauth2": "^0.1.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.71.4"
  },
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-logger": "^2.0.2",
    "@types/koa-passport": "^2.0.4",
    "@types/koa-router": "^7.0.22",
    "@types/koa-session": "^3.0.3",
    "@types/koa-static": "^3.0.0",
    "@types/node": "^8.0.24",
    "@types/passport-google-oauth2": "^0.1.1",
    "@types/react": "^16.0.2",
    "@types/react-dom": "^15.5.3",
    "@types/react-hot-loader": "^3.0.2",
    "@types/webpack": "^3.0.9",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "img-loader": "^2.0.0",
    "jest": "^20.0.4",
    "postcss": "^6.0.3",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "react-dev-utils": "^3.1.1",
    "react-error-overlay": "^1.0.8",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.3.3",
    "tslint": "^5.4.3",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.5.0",
    "webpack-node-externals": "^1.6.0"
  }
}
