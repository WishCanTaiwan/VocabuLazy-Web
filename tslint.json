{
  "extends": "tslint:latest",
  "rules": {
    "linebreak-style": [
      true,
      "LF"
    ],
    "max-line-length": false,
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "no-switch-case-fall-through": true,
    "no-submodule-imports": false,
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "radix": false
  }
}
